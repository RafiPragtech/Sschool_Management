<odoo>
    <data>
            <!-- Action -->
            <record id="schhol_model_menu_action" model="ir.actions.act_window">
                <field name="name">school</field>
                <field name="res_model">school.school</field>
                <field name="view_mode">tree,form</field>
                <field name="help" type="html">
                    <p class="o_view_nocontent_smiling_face">
                        Model
                    </p>
                </field>
            </record>
    
            <!-- tree_view -->
    
            <record id="schhol_model_menu_tree" model="ir.ui.view">
                <field name="name">school.tree</field>
                <field name="model">school.school</field>
                <field name="arch" type="xml">
                    <tree string="Model">
                        <field name="name" />
                        <field name="age"/>
                        <field name="classe" />
                        
                    </tree>
                </field>
            </record>
    
            <!-- list_view -->
            <record id="schhol_model_menu_form" model="ir.ui.view">
                <field name="name">school.form</field>
                <field name="model">school.school</field>
                <field name="arch" type="xml">
                    <form string="Model">
                        <sheet>
                            <group>
                                <group>
                                    <field name="name" />
                                    <field name="age"/>
                                    <field name="classe" />
                                </group>
                            </group>
                        </sheet>
                    </form>
                </field>
    
            </record>
    </data>
</odoo>