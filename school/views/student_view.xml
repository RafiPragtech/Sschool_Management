<odoo>
    <data>
            <!-- Action -->
            <record id="schhol_student_menu_action" model="ir.actions.act_window">
                <field name="name">Student</field>
                <field name="res_model">school.student</field>
                <field name="view_mode">tree,form</field>
                <field name="help" type="html">
                    <p class="o_view_nocontent_smiling_face">
                        Student
                    </p>
                </field>
            </record>
    
            <!-- tree_view -->
    
            <record id="schhol_student_menu_tree" model="ir.ui.view">
                <field name="name">student.tree</field>
                <field name="model">school.student</field>
                <field name="arch" type="xml">
                    <tree string="student">
                        <field name="name" />
                        <field name="dob"/>
                        <field name="age" />
                        <field name="place"/>
                        
                    </tree>
                </field>
            </record>
    
            <!-- list_view -->
            <record id="schhol_student_menu_form" model="ir.ui.view">
                <field name="name">student.form</field>
                <field name="model">school.student</field>
                <field name="arch" type="xml">
                    <form string="Student">
                        <sheet>
                            <group>
                                <group>
                                    <field name="name" />
                                    <field name="dob"/>
                                    <field name="age" />
                                    <field name="place"/>
                                    
                                </group>
                            </group>
                        </sheet>
                    </form>
                </field>
    
            </record>
    </data>
</odoo>